{
    "env": {
    "es6": true,
        "node": true,
        "browser": true,
        "commonjs": true,
        "jest": true
},
    "globals": {
    "localStorage": true,
        "fetch": true
},
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
],
    "parser": "babel-eslint",
    "parserOptions": {
    "ecmaVersion": 2017,
        "sourceType": "module"
},
    "plugins": [
    "react",
    "react-hooks"
],
    "rules": {
    "indent": [
        "error",
        4,
        { "SwitchCase": 1 }
    ],
        "linebreak-style": [
        "error",
        "unix"
    ],
        "quotes": [
        "error",
        "single"
    ],
        "semi": [
        "error",
        "always"
    ],
        "no-console":"off",
        "react/no-unused-prop-types": "error",
        "react-hooks/rules-of-hooks": "error",
        "react/display-name": [0, { "ignoreTranspilerName": true }]
},
    "settings": {
    "react": {
        "version": "detect"
    }
}
}
